{
  "name": "capco-angular-5-data-table",
  "version": "0.1.1",
  "license": "",
  "private": false,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test -sm=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:docs": "compodoc ./libs -p src/tsconfig.packages.json -s",
    "build:app": "cpr dist/angular5-data-table src/angualar5-datatable --overwrite && ng build --app packages && rimraf src/angular5-data-table",
    "build:app:prod": "cpr dist/angular5-data-table src/angular5-data-table --overwrite && ng build --prod --named-chunks=true --app packages && rimraf src/angular5-data-table",
    "build:libs:datatable": "ng-packagr -p libs/datatable/package.json",
    "publish:libs:datatable": "npm publish dist/angular5-data-table"
  },
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "bootstrap": "^4.0.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.12.0",
    "lodash-es": "^4.17.4",
    "ngx-md": "^3.1.1",
    "popper.js": "^1.14.3",
    "rxjs": "^5.5.6",
    "tslint-angular": "^1.1.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular-devkit/core": "0.3.2",
    "@angular-devkit/schematics": "0.3.2",
    "@angular/cli": "1.7.1",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@compodoc/compodoc": "^1.0.5",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^8.0.47",
    "codelyzer": "^4.2.0",
    "cpr": "^3.0.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.1.0",
    "node-sass": "4.7.2",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.1",
    "ts-node": "~4.1.0",
    "tsickle": "^0.26.0",
    "tslint": "~5.9.1",
    "typescript": "2.6.1"
  }
